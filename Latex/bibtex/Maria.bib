@article{Garcia2017,
abstract = {BACKGROUND Medication errors are frequent and may cause harm to patients and increase healthcare expenses. AIM To explore whether a new labelling influences time and errors when preparing medications in accordance with medication charts in an experimental setting. METHOD We carried out an uncontrolled before and after study with 3 months inbetween experiments. Phase I used original labelling and phase II used new generic labelling. We set up an experimental medicine room, simulating a real-life setting. Twenty-five nurses and ten pharmacy technicians participated in the study. We asked them to prepare medications in accordance with medication charts, place packages on a desk and document the package prepared. We timed the operation. Participants were asked to prepare medications in accordance with as many charts as possible within 30 min. RESULTS Nurses prepared significantly more medication charts with the generic labelling compared with the original 3.3 versus 2.6 (p=0.009). Mean time per medication chart was significantly lower with the generic labelling 6.9 min/chart versus 8.5 min/chart (p{\textless}0.001). Pharmacy technicians were significantly faster than the nurses in both phase I (6.8 min/chart vs 9.5 min/chart; p{\textless}0.001) and phase II (6.1 min/chart vs 7.2 min/chart; p=0.013). The number of errors was low and not significantly different between the two labellings, with errors affecting 9.1{\%} of charts in phase I versus 6.5{\%} in phase II (p=0.5). CONCLUSIONS A new labelling of medication packages with prominent placement of the active substance(s) and strength(s) in the front of the medication package may reduce time for nurses when preparing medications, without increasing medication errors.},
author = {Garcia, Beate Hennie and Elenjord, Renate and Bjornstad, Camilla and Halvorsen, Kjell Hermann and Hortemo, Sigurd and Madsen, Steinar},
doi = {10.1136/bmjqs-2016-006422},
file = {:Users/Maria/Desktop/Artikler/Look-a-like3GOD!.pdf:pdf},
isbn = {0340-6245 (Print)$\backslash$r0340-6245 (Linking)},
issn = {20445415},
journal = {BMJ Quality and Safety},
pmid = {28432189},
title = {{Safety and efficiency of a new generic package labelling: A before and after study in a simulated setting}},
year = {2017}
}
@article{Kaushal2002,
abstract = {Medication errors occur frequently and have significant clinical and financial consequences. Several types of information technologies can be used to decrease rates of medication errors. Computerized physician order entry with decision support significantly reduces serious inpatient medication error rates in adults. Other available information technologies that may prove effective for inpatients include computerized medication administration records, robots, automated pharmacy systems, bar coding, "smart" intravenous devices, and computerized discharge prescriptions and instructions. In outpatients, computerization of prescribing and patient oriented approaches such as personalized web pages and delivery of web based information may be important. Public and private mandates for information technology interventions are growing, but further development, application, evaluation, and dissemination are required.},
author = {Kaushal, R},
doi = {10.1136/qhc.11.3.261},
file = {:Users/Maria/Documents/UNI/ST9/Litteraturs{\o}gning/Teknologi/Kaushal2002.pdf:pdf},
isbn = {1475-3898},
issn = {14753898},
journal = {Quality and Safety in Health Care},
pmid = {12486992},
title = {{Information technology and medication safety: what is the benefit?}},
url = {http://qualitysafety.bmj.com/lookup/doi/10.1136/qhc.11.3.261},
year = {2002}
}
@article{Croskerry2011,
abstract = {A major amount of our time working in clinical practice involves thinking and decision making. Perhaps it is because decision making is such a commonplace activity that it is assumed we can all make effective decisions. However, this is not the case and the example of diagnostic error supports this assertion. Until quite recently there has been a general nihilism about the ability to change the way that we think, but it is now becoming accepted that if we can think about, and understand, our thinking processes we can improve our decision making, including diagnosis. In this paper we review the dual process model of decision making and highlight ways in which decision making can be improved through the application of this model to our day-to-day practice and by the adoption of de-biasing strategies and critical thinking.},
archivePrefix = {arXiv},
arxivId = {arXiv:1011.1669v3},
author = {Croskerry, P. and Nimmo, G. R.},
