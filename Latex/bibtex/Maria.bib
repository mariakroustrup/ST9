journal = {Academic Pediatrics},
keywords = {children,patient safety,prescription error},
pmid = {20541485},
publisher = {Elsevier Ltd},
title = {{Using pharmacy data to screen for look-alike, sound-alike substitution errors in pediatric prescriptions}},
url = {http://dx.doi.org/10.1016/j.acap.2010.04.024},
year = {2010}
}
